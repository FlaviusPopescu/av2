            \PY{n}{perc} \PY{p}{=} \PY{n}{ii}\PY{o}{/}\PY{p}{(}\PY{n+nb}{size}\PY{p}{(}\PY{n}{files}\PY{p}{,}1\PY{p}{)} \PY{o}{*} 2 \PY{o}{+} 4\PY{p}{)}\PY{p}{;}
